---
import Containter from "../components/Container.astro";
import BaseLayout from "../layouts/BaseLayout.astro";
import HeroInner from "../components/PageBanner.astro";
import ContactForm from "../components/ContactForm.astro";
import CtaBanner from "../components/CtaBanner.astro";

const title = "Contact Us";
const desc = "";
const image = "/page-banner-image.webp";
---

<BaseLayout title="Contact Us" description="Contact Us">
  <HeroInner title={title} desc={desc} image={image} />
  <Containter>
    <ContactForm />
    <CtaBanner />
  </Containter>
</BaseLayout>
